{
  "compilerOptions": {
    "target": "es2016",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmitOnError": false,
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "./",
    "paths": {
      "npm:*": ["node_modules/*", "*"],
      "*": ["node_modules/*"]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/services/*.ts"
  ],
  "exclude": [
    "node_modules/**/*.ts"
  ]
}
