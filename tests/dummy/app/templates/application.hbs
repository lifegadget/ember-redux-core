<div class="ember-redux-core">

  <div class="heading">
    <div class="logo">
      <img src="images/ember-redux.png" alt="logo" width='175px'>
    </div>
    <div class="text">
      <h1 id="title">ember-redux-core</h1>
      <div class="sub-heading">Bringing the core REDUX state machine to Ember</div>
    </div>
  </div>


  <div class="page">
  <div class="left-side">
    <h2>Docs</h2>
    <ul>
      <li>{{#link-to 'index' class='count-link'}}Overview{{/link-to}}</li>
      <li>{{#link-to 'the-store' class='count-link'}}The Store{{/link-to}}</li>
      <li>{{#link-to 'reducers' class='count-link'}}Reducers{{/link-to}}</li>
      <li>{{#link-to 'middleware' class='count-link'}}Middleware{{/link-to}}</li>
      <li>{{#link-to 'enhancers' class='count-link'}}Enhancers{{/link-to}}</li>
    </ul>
    <h2>Demos</h2>
    <ul>
      <li>{{#link-to 'count' class='count-link'}}Count<br/><small>(numeric state example)</small>{{/link-to}}</li>
      <li>{{#link-to 'users' class='users-link'}}Users<br/><small>(array state example)</small>{{/link-to}}</li>
      <li>{{#link-to 'count' class='-link'}}Identity<br/><small>(hash state example)</small>{{/link-to}}</li>
    </ul>
  </div>

  <div class="right-side">
    {{outlet}}
  </div>

  </div>





  {{!--
